    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "morgan": "^1.10.0",
    "node-sass": "^8.0.0",
    "supertest": "^6.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/hbs": "^4.0.1",
    "@types/jasmine": "^4.3.0",
