  "scripts": {
    "test": "npx tsc && jasmine",
    "dev": "nodemon ./src/index.ts",
    "clean": "rimraf build/",
    "build": "yarn clean && npm run post-build && npx tsc",
    "post-build": "xcopy .\\src\\App\\Views\\* .\\build\\App\\Views\\ /E /H /C /I /Y",
    "sass": "node-sass -w public/style/scss -o public/style/css",
    "start": "npm run build && nodemon build/index.js",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
